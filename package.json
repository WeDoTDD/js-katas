    "@babel/core": "^7.21.4",
    "@babel/plugin-proposal-object-rest-spread": "^7.20.7",
    "@babel/plugin-transform-runtime": "^7.21.4",
    "@babel/preset-env": "^7.21.4",
    "@babel/register": "^7.21.0",
    "@babel/runtime": "^7.21.0",
    "@jest/globals": "^29.5.0",
    "@types/jest": "^29.5.1",
    "babel-jest": "^29.5.0",
    "chai": "^4.3.7",
    "jest": "^29.5.0",
    "mocha": "^10.2.0",
    "ts-jest": "^29.1.0",
    "typescript": "^5.0.4"
  }
