  "license": "",
  "devDependencies": {
    "@babel/cli": "^7.13.16",
    "@babel/core": "^7.13.16",
    "@babel/plugin-proposal-object-rest-spread": "^7.13.8",
    "@babel/plugin-transform-runtime": "^7.13.15",
    "@babel/preset-env": "^7.13.15",
    "@babel/register": "^7.13.16",
    "@babel/runtime": "^7.13.17",
    "@jest/globals": "^26.6.2",
    "@types/jest": "^26.0.22",
    "babel-jest": "^26.6.3",
    "chai": "^4.3.4",
    "jest": "^26.6.3",
    "mocha": "^8.3.2",
    "ts-jest": "^26.5.5",
    "typescript": "^4.2.4"
  }
